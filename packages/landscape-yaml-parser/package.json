  },
  "dependencies": {
    "maketypes": "^1.1.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@frontside/eslint-config": "^3.1.0",
    "@frontside/typescript": "^4.0.0",
    "@types/node": "^20.10.4",
    "ts-node": "^10.9.2"
  }
}
